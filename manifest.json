{
  "manifest_version": 2,
  "name": "Safari Vibe Ultimate",
  "version": "1.2",
  "description": "极简、Safari风格的Firefox新标签页。",
"permissions": [
    "topSites",
    "storage",
    "sessions",
    "tabs",
    "history",
    "bookmarks",   
    "*://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "safari-vibe-ultimate@user.addon",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "chrome_settings_overrides": {
    "homepage": "newtab.html"
  }
}